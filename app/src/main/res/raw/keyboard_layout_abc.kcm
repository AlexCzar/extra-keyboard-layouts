    shift, capslock:                    'B'
}

key E {
    label:                              'C'
    base:                               'c'
    shift, capslock:                    'C'
    alt:                                '\u00e7'
    shift+alt:                          '\u00c7'
}

key R {
    label:                              'D'
    base:                               'd'
    shift, capslock:                    'D'
}

key T {
    label:                              'E'
